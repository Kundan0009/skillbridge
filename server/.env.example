# API Keys - NEVER expose these in frontend
GEMINI_API_KEY=your_gemini_api_key_here

# Database
MONGO_URI=mongodb+srv://username:password@cluster.mongodb.net/skillbridge?retryWrites=true&w=majority

# Security
JWT_SECRET=your_super_secret_jwt_key_here_make_it_long_and_random

# Server Config
CLIENT_URL=http://localhost:4000
PORT=9000
NODE_ENV=development

# Optional MongoDB Init (for Docker)
MONGO_INIT_USER=skillbridge_user
MONGO_INIT_PASSWORD=secure_password_here